{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Annotation</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Iduser</th>
                <td>{{ entity.idUser }}</td>
            </tr>
            <tr>
                <th>Annotationtype</th>
                <td>{{ entity.AnnotationType }}</td>
            </tr>
            <tr>
                <th>Annotationcontent</th>
                <td>{{ entity.AnnotationContent }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('annotation') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('annotation_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
