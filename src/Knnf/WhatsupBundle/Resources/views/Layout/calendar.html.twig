		<div id="content">
			<div id="content-header">
				<h1>Calendar</h1>
				<div class="btn-group">
					<a class="btn" title="Manage Files"><i class="glyphicon glyphicon-file"></i></a>
					<a class="btn" title="Manage Users"><i class="glyphicon glyphicon-user"></i></a>
